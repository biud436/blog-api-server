    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/src/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^@app/libs/env(|/.*)$": "<rootDir>/libs/env/src/$1",
            "^src/(.*)$": "<rootDir>/src/$1"
        }
    }
